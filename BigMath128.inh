{
	return (u128)x * y;
}

CAT_INLINE u128 u128_prod_sum(const u64 x, const u64 y, const u64 z)
{
	return (u128)x * y + z;
}

CAT_INLINE u128 u128_prod_63_sum(const u64 x, const u64 y, const u64 z)
{
	return (u128)x * y + z;
}

CAT_INLINE u128 u128_lshift_sum(const u64 x, int shift, const u64 z)
